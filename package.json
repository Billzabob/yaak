{
  "name": "tauri-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rsw build && vite build",
    "dev": "vite",
    "lint": "tsc && eslint . --ext .ts,.tsx",
